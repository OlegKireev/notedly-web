{
  "name": "notic-web",
  "version": "0.1.0",
  "description": "Note managment single page application",
  "main": "src/index.js",
  "scripts": {
    "start": "yarn dev",
    "dev": "parcel src/index.html",
    "final": "parcel final/index.html",
    "deploy:src": "parcel build src/index.html --public-url ./",
    "deploy:final": "parcel build final/index.html --public-url ./",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "deploy": "git push && git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OlegKireev/notedly-web.git"
  },
  "keywords": [
    "react",
    "graphql",
    "apollo",
    "styled-components",
    "web application"
  ],
  "author": "Oleg Kireev <flucky@mail.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OlegKireev/notedly-web/issues"
  },
  "homepage": "https://github.com/OlegKireev/notedly-web#readme",
  "dependencies": {
    "@apollo/client": "3.5.7",
    "@types/react": "^17.0.38",
    "apollo-link-context": "1.0.20",
    "date-fns": "1.30.1",
    "graphql": "^16.2.0",
    "normalize.css": "8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "7.1.2",
    "react-router-dom": "5.1.2",
    "styled-components": "5.3.3"
